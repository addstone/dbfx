<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Button?>
<?import java.net.URL?>
<?import javafx.scene.control.ScrollPane?>
<ScrollPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="600.0" prefWidth="700.0" fitToHeight="true" fitToWidth="true">
    <stylesheets>
        <URL value="@/css/edit_con_style.css"/>
    </stylesheets>
    <BorderPane>
        <bottom>
            <HBox styleClass="bottom-box">
                <Button text="%label.cancel" onAction="#cancel"/>
                <Button text="%label.save" fx:id="save" onAction="#saveEdit"/>
            </HBox>
        </bottom>
    </BorderPane>
</ScrollPane>
